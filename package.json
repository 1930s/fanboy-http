{
  "name": "fanboy-http",
  "version": "2.1.0",
  "description": "fanboy HTTP API",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "standard",
    "test": "tap test/*.js",
    "posttest": "rm -rf /tmp/fanboy-http-[1-9]*",
    "start": "node start.js > /dev/null & echo $! > .pid",
    "stop": "kill $(cat .pid)"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelnisi/fanboy-http.git"
  },
  "keywords": [
    "fanboy",
    "http"
  ],
  "bugs": {
    "url": "https://github.com/michaelnisi/fanboy-http/issues"
  },
  "homepage": "https://github.com/michaelnisi/fanboy-http",
  "engines": {
    "node": ">=6"
  },
  "author": "Michael Nisi <michael.nisi@gmail.com> (http://michaelnisi.com/)",
  "dependencies": {
    "bunyan": "^1.8.5",
    "fanboy": "^5.0.3",
    "http-hash": "^2.0.0",
    "mkdirp": "^0.5.1",
    "negotiator": "^0.6.1"
  },
  "devDependencies": {
    "lino": "^1.0.0",
    "nock": "^9.0.7",
    "rimraf": "^2.6.1",
    "standard": "^8.6.0",
    "tap": "^10.2.2"
  },
  "license": "MIT"
}
