  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michaelnisi/fanboy-http/issues"
  },
  "homepage": "https://github.com/michaelnisi/fanboy-http",
  "dependencies": {
    "bunyan": "0.23.x",
    "fanboy": "0.2.x",
    "leveldown": "0.10.x",
    "levelup": "0.18.x",
    "mkdirp": "0.5.x",
    "routes": "1.2.x"
  },
  "devDependencies": {
    "tap": "0.4.x"
  },
  "engines": {
    "node": ">=0.10"
  }
