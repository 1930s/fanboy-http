{
  "name": "fanboy-http",
  "version": "0.0.0",
  "description": "fanboy HTTP API",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "export NODE_TEST=1 ; tap --tap test/*.js",
    "start": "node start.js > /dev/null & echo $! > .pid",
    "stop": "kill `< .pid`"
  },
  "repository": {
    "type": "git",
