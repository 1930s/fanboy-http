{
  "name": "fanboy-http",
  "version": "2.0.4",
  "description": "fanboy HTTP API",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_TEST=1 tap -b test/*.js",
    "posttest": "rm -rf /tmp/fanboy-http-[1-9]*",
    "start": "node start.js > /dev/null & echo $! > .pid",
    "stop": "kill $(cat .pid)"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelnisi/fanboy-http.git"
  },
  "keywords": [
    "fanboy",
    "http"
  ],
  "bugs": {
    "url": "https://github.com/michaelnisi/fanboy-http/issues"
  },
  "homepage": "https://github.com/michaelnisi/fanboy-http",
  "engines": {
    "node": ">=0.10.x"
  },
  "author": "Michael Nisi <michael.nisi@gmail.com> (http://michaelnisi.com/)",
  "dependencies": {
    "bunyan": "^1.5.1",
    "fanboy": "^3.1.3",
    "http-hash": "^2.0.0",
    "mkdirp": "^0.5.1",
    "negotiator": "^0.6.0"
  },
  "devDependencies": {
    "lino": "^1.0.0",
    "nock": "^4.0.0",
    "rimraf": "^2.4.4",
    "tap": "^2.2.1"
  },
  "license": "MIT"
}
